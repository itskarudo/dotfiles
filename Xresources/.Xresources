!-- Xft settings -- !
!Xft.antialias: 1

Xft.hinting: 1

Xft.autohint: 0

Xft.hintstyle: hintslight

Xft.rgba: rgb

Xft.lcdfilter: lcddefault

Xft.dpi:        96
Xft.antialias:  true
Xft.hintstyle:  hintfull

! special
*.foreground:   #bfddb2
*.background:   #2a2f33
*.cursorColor:  #bfddb2

! black
*.color0:       #504945
*.color8:       #504945

! red
*.color1:       #ea5252
*.color9:       #ea5252

! green
*.color2:       #ACB765
*.color10:      #ACB765

! yellow
*.color3:       #c1bf89
*.color11:      #c1bf89

! blue
*.color4:       #82abbc
*.color12:      #82abbc

! magenta
*.color5:       #b18a97
*.color13:      #b18a97

! cyan
*.color6:       #88b482
*.color14:      #89b482

! white
*.color7:       #bfddb2
*.color15:      #bfddb2
