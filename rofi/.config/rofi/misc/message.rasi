/* Confirm Dialog */


* {
    background-color:       @background;
    text-color:             @foreground;
	font:				 	"Comfortaa 12";
}

window {
    width:      			360px;
    padding:    			25px;
    border:					0px;
    border-radius:          0px;
    border-color:           @color6;
    location:               center;
    y-offset:               -20px;
}

entry {
    expand: 				true;
    text-color:				@color6;
}
